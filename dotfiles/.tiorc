# Defaults
baudrate = 115200
databits = 8
parity = none
stopbits = 1
color = 10

[ftdi]
tty = /dev/serial/by-id/usb-FTDI_TTL232R-3V3_FTGQVXBL-if00-port0
baudrate = 9600
no-autoconnect = enable
log = enable
log-file = ftdi.log
color = 12

[usb devices]
pattern = usb([0-9]*)
tty = /dev/ttyUSB%s
color = 13i

[xil]
pattern = xil([0-9]{1})
tty = /dev/serial/by-id/usb-Xilinx_JTAG+3Serial_41678-if0%s-port0
color = 14

